gcc -Wall -c src/inout.c -o inout.o && 
gcc -Wall -c src/maths.c -o maths.o &&
gcc -Wall -c src/error.c -o error.o &&
gcc -Wall -c src/radiere.c -o radiere.o &&
gcc -Wall -c src/drumuire.c -o drumuire.o &&
gcc -Wall -c src/main.c -o main.o &&
gcc -Wall inout.o maths.o error.o radiere.o drumuire.o main.o -o topo &&
echo Execution starts &&
./topo -d drumuire.in ex.out
